
# Binance API Configuration
VITE_BINANCE_API_KEY=your_binance_api_key_here
VITE_BINANCE_SECRET_KEY=your_binance_secret_key_here
VITE_BINANCE_TESTNET=false

# Trading Configuration
VITE_TRADING_MODE=production
VITE_MIN_TRADE_AMOUNT=20
VITE_MAX_TRADE_AMOUNT=1000
VITE_DEFAULT_RISK_PER_TRADE=0.5
VITE_MAX_DAILY_LOSS=2.0
VITE_ENABLE_REAL_TRADING=true

# Security
VITE_ENABLE_LOGGING=true
VITE_LOG_LEVEL=info

# Monitoring
VITE_ENABLE_MONITORING=true
