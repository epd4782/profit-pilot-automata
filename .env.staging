
# Binance API Configuration (Testnet)
VITE_BINANCE_API_KEY=your_testnet_api_key_here
VITE_BINANCE_SECRET_KEY=your_testnet_secret_key_here
VITE_BINANCE_TESTNET=true

# Trading Configuration
VITE_TRADING_MODE=staging
VITE_MIN_TRADE_AMOUNT=10
VITE_MAX_TRADE_AMOUNT=100
VITE_DEFAULT_RISK_PER_TRADE=1.0
VITE_MAX_DAILY_LOSS=5.0
VITE_ENABLE_REAL_TRADING=false

# Security
VITE_ENABLE_LOGGING=true
VITE_LOG_LEVEL=debug

# Monitoring
VITE_ENABLE_MONITORING=true
